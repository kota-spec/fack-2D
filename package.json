{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "yarn dev",
    "dev": "gulp build:dev",
    "prebuild": "rimraf ./docs",
    "build": "cross-env NODE_ENV=docs gulp build",
    "browser": "browser-sync start -s 'docs'",
    "iconfont": "gulp iconfont:build && yarn lint:style",
    "lint:html": "gulp pug:lint",
    "lint:script": "eslint --fix --ext .js --ignore-path .gitignore .",
    "lint:style": "stylelint --fix --ignore-path .stylelintignore \"**/*.scss\"",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": ["yarn lint:script", "git add --force"],
    "*.scss": ["yarn lint:style", "git add --force"]
  },
  "author": "kota",
  "license": "ISC",
  "browserslist": ["last 2 versions", "ie 9"],
